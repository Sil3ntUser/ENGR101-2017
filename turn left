#include <stdio.h>
#include <time.h>
#include "E101.h"


int main(){
init();

    set_motor(1,-50);// motor on left
    sleep1(0,500000);
    
    set_motor(2,50);// motor on right
    sleep1(0,500000);
    
    set_motor(1,0);// stops
    sleep1(0,500000);
    
    set_motor(2,0);// stops
    sleep1(0,500000);
    
return 0;
}
